<Window x:Class="Luna_X.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:Core="clr-namespace:Luna_X.Core"
        xmlns:MiscControls="clr-namespace:Luna_X.Controls.Misc"
        xmlns:ScriptHubControls="clr-namespace:Luna_X.Controls.Script_Hub"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:Luna_X"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialdesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        Title="Luna X - Nebula Client"
        UseLayoutRounding="True"
        Topmost="True"
        Width="1080"
        Height="595"
        MinWidth="890"
        MinHeight="500"
        AllowsTransparency="True"
        Background="Transparent"
        AllowDrop="True"
        ResizeMode="CanResize"
        WindowStartupLocation="CenterScreen"
        WindowStyle="None"
        mc:Ignorable="d"
        Loaded="Window_Loaded"
        TextOptions.TextFormattingMode="Ideal"
        MouseLeftButtonDown="Window_MouseLeftButtonDown">

    <Window.Resources>
        <Core:RectangleConverter x:Key="RC" />
        <FontFamily x:Key="Oswald">/Luna X;component/Graphics/Fonts/#Oswald</FontFamily>
        <FontFamily x:Key="OswaldSemilight">/Luna X;component/Graphics/Fonts/#Oswald Light</FontFamily>
        <!--this is literally for maximise icons only-->
        <FontFamily x:Key="FluentIcon">/Luna X;component/Graphics/Fonts/Icon/#Segoe Fluent Icons</FontFamily>
        <FontFamily x:Key="AwesomeIcon">/Luna X;component/Graphics/Fonts/Icon/#Font Awesome 6 Free Regular</FontFamily>
    </Window.Resources>

    <WindowChrome.WindowChrome>
        <WindowChrome CaptionHeight="0"
                      ResizeBorderThickness="14"
                      CornerRadius="8"
                      GlassFrameThickness="-1"
                      UseAeroCaptionButtons="True" />
    </WindowChrome.WindowChrome>

    <Border x:Name="BorderBackground"
            Background="#1B1A1E" 
            BorderBrush="#EF41434A"
            CornerRadius="8"
            Margin="12"
            BorderThickness="1.2">

        <Border.Effect>
            <DropShadowEffect ShadowDepth="0"
                              x:Name="Shadow"
                              Color="#090909"
                              BlurRadius="12"
                              Opacity="1"/>
        </Border.Effect>

        <Grid>

            <!--Disable This In Designer view since this is a build only function and will desttroy the designer preview-->
            <!--<Grid.Clip>
                <RectangleGeometry RadiusX="8" RadiusY="8">
                    <RectangleGeometry.Rect>
                        <MultiBinding Converter="{StaticResource RC}">
                            <Binding Path="ActualWidth" ElementName="BorderBackground"/>
                            <Binding Path="ActualHeight" ElementName="BorderBackground"/>
                        </MultiBinding>
                    </RectangleGeometry.Rect>
                </RectangleGeometry>
            </Grid.Clip>-->

            <!--Main Window-->
            <Grid>
                    <Grid.Effect>
                        <BlurEffect x:Name="Blurrer"
                                    Radius="0"
                                    KernelType="Gaussian"/>
                    </Grid.Effect>
                    
                    <!--#region Topbar-->
                    <Grid x:Name="Topbar"
                          Height="35"
                          Background="Transparent"
                          MouseLeftButtonDown="Drag"
                          VerticalAlignment="Top">

                        <Image x:Name="Icon"
                               Margin="7.5 0 0 -5"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Center"
                               Height="36"
                               Source="/Luna X;component/Graphics/Images/Luna X Logo V2.png"
                               RenderOptions.BitmapScalingMode="HighQuality"/>

                        <StackPanel x:Name="Controls"
                                    Height="35"
                                    Margin="3,0"
                                    FlowDirection="RightToLeft"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Top"
                                    Orientation="Horizontal">

                            <!--padding is literally from playing around to give that specific effect idk how to describe-->
                            <Button Width="38" 
                                    Height="35"
                                    Background="Transparent"
                                    BorderBrush="Transparent" 
                                    Content="&#xE8BB;"
                                    FontFamily="Segoe MDL2 Assets" 
                                    FontSize="10"
                                    Foreground="White" 
                                    BorderThickness="0"
                                    HorizontalAlignment="Center" 
                                    VerticalAlignment="Center"
                                    Padding="0"
                                    Panel.ZIndex="100"
                                    Style="{StaticResource CloseButton}"
                                    Click="Close_Click" />

                            <!-- Maximize Button -->
                            <Button x:Name="MaximiseButton"
                                    Width="38"
                                    Height="35"
                                    Background="Transparent" 
                                    BorderBrush="Transparent"
                                    Content="square"
                                    FontFamily="/Luna X;component/Graphics/Fonts/Icon/#Font Awesome 6 Free Regular"
                                    FontSize="12"
                                    Padding="0 1.0 1 0"
                                    Foreground="White"
                                    BorderThickness="0" 
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Panel.ZIndex="100"
                                    Style="{StaticResource BasicButton}"
                                    TextOptions.TextFormattingMode="Display"
                                    Click="MaximiseClick"/>

                            <!-- Minimize Button -->
                            <Button Width="38" 
                                    Height="35"
                                    Background="Transparent"
                                    BorderBrush="Transparent" 
                                    Content="&#xE738;"
                                    FontFamily="Segoe MDL2 Assets" 
                                    FontSize="11"
                                    Padding="-1 2 0 0"
                                    Foreground="White"
                                    BorderThickness="0"
                                    HorizontalAlignment="Center" 
                                    VerticalAlignment="Center"
                                    Panel.ZIndex="100"
                                    Style="{StaticResource BasicButton}"
                                    Click="MinimizeClick"/>

                            <TextBlock x:Name="RedistrubutorText"
                                       Text="NebulaSoftworks.Framer.AI"
                                       Foreground="WhiteSmoke"
                                       Margin="4 0 0 0"
                                       FontFamily="/Luna X;component/Graphics/Fonts/#Poppins Medium"
                                       VerticalAlignment="Center"
                                       FontSize="12"/>

                        </StackPanel>

                    </Grid>
                    <!--#endregion-->

                    <!--#region Sidebar Navigation-->

                    <Grid x:Name="Sidebar"
                          Width="49"
                          Margin="0 38 0 0"
                          HorizontalAlignment="Left">

                        <StackPanel x:Name="Main"
                                    Margin="0 0 0 86">

                            <RadioButton x:Name="HomeRadioBtn"
                                         Height="43"
                                         BorderBrush="{DynamicResource Accent}"
                                         FontFamily="/Luna X;component/Graphics/Fonts/Icon/#Segoe Fluent Icons"
                                         Tag="&#xea8a;"
                                         FontSize="15"
                                         Style="{StaticResource TabBarFluent}"
                                         GroupName="Tabs"
                                         IsChecked="True"
                                         Checked="NavigationClick"/>

                            <RadioButton x:Name="ExecRadioBtn"
                                         Height="43"
                                         BorderBrush="{DynamicResource Accent}"
                                         Tag="code"
                                         FontSize="15"
                                         Style="{StaticResource TabBar}"
                                         GroupName="Tabs"
                                         Checked="NavigationClick"/>

                            <RadioButton x:Name="CloudRadioBtn"
                                         Height="43"
                                         BorderBrush="{DynamicResource Accent}"
                                         Tag="cloud"
                                         FontSize="15"
                                         Style="{StaticResource TabBar}"
                                         GroupName="Tabs"
                                         Checked="NavigationClick"/>

                            <RadioButton x:Name="QuickRadioBtn"
                                         Height="43"
                                         BorderBrush="{DynamicResource Accent}"
                                         Tag=""
                                         FontSize="15"
                                         Style="{StaticResource TabBar}"
                                         GroupName="Tabs"
                                         Checked="NavigationClick"/>

                            <RadioButton x:Name="PluginRadioBtn"
                                         Height="43"
                                         BorderBrush="{DynamicResource Accent}"
                                         FontFamily="/Luna X;component/Graphics/Fonts/Icon/#Material Icons Round"
                                         Tag="&#xe87b;"
                                         FontSize="17"
                                         Style="{StaticResource TabBarFluent}"
                                         GroupName="Tabs"
                                         Checked="NavigationClick"/>

                            <RadioButton x:Name="FriendsRadioBtn"
                                         Height="43"
                                         BorderBrush="{DynamicResource Accent}"
                                         Tag="&#xf500;"
                                         FontSize="15"
                                         Style="{StaticResource TabBar}"
                                         GroupName="Tabs"
                                         Checked="NavigationClick"/>

                        </StackPanel>

                        <RadioButton x:Name="SettingsRadioBtn"
                                     VerticalAlignment="Bottom"
                                     Margin="0 0 0 5"
                                     Padding="2 0 0 0"
                                     Height="43"
                                     BorderBrush="{DynamicResource Accent}"
                                     Tag="&#xe8b8;"
                                     FontFamily="/Luna X;component/Graphics/Fonts/Icon/#Material Icons Round"
                                     FontSize="17"
                                     Style="{StaticResource TabBarFluent}"
                                     GroupName="Tabs"
                                     Checked="NavigationClick"/>

                        <Ellipse x:Name="Status"
                                 Height="12"
                                 Width="12"
                                 VerticalAlignment="Bottom"
                                 Margin="0 0 0 53"
                                 Fill="Khaki">
                            <Ellipse.Effect>
                                <DropShadowEffect x:Name="StatusShadow"
                                                  ShadowDepth="0"
                                                  BlurRadius="12"
                                                  Color="Khaki"/>
                            </Ellipse.Effect>

                        </Ellipse>
                        <!--PaleVioletRed, DarkSeaGreen, Khaki-->

                    </Grid>

                    <!--#endregion-->

                    <Border x:Name="ContentHolder"
                            CornerRadius="8 0 8 0"
                            Margin="49 35 0 0"
                            BorderThickness="1 1 0 0"
                            BorderBrush="#4E3e4142"
                            Background="#131215">

                        <Grid>

                            <Grid x:Name="MainTabs" >

                                <Grid.Effect>
                                    <!--Blur to make home tab look like comet-->
                                    <BlurEffect x:Name="BlurMain" 
                                                Radius="0" 
                                                KernelType="Gaussian"/>
                                </Grid.Effect>

                                <!--#region Executor-->
                                <Grid x:Name="Executor"
                                      Visibility="Collapsed"
                                      Opacity="0">


                                    <MiscControls:Tab_System x:Name="TabSystemz"
                                                             Margin="200,5,5,45"/>

                                    <Border x:Name="WorkspaceBorder"
                                            Background="#16383838"
                                            BorderBrush="#5F41434A"
                                            CornerRadius="4"
                                            Width="190"
                                            BorderThickness="1.2"
                                            HorizontalAlignment="Left"
                                            Margin="5,5,5,5">

                                            <Grid>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="40"/>
                                                    <RowDefinition/>
                                                </Grid.RowDefinitions>

                                                <TextBlock x:Name="WorkSpaceTitle"
                                                           Text="Workspace Explorer"
                                                           Foreground="LightGray"
                                                           Margin="8 0 0 0"
                                                           FontFamily="/Luna X;component/Graphics/Fonts/#Poppins Medium"
                                                           VerticalAlignment="Center"
                                                           FontSize="13"/>

                                                <Border Height="1"
                                                        VerticalAlignment="Bottom"
                                                        Background="#2F41434A"
                                                        BorderBrush="Transparent"/>

                                                <TextBox x:Name="WorkspaceSearch"
                                                         Grid.Row="1"
                                                         Padding="30 0 0 0"
                                                         Style="{StaticResource placeHolder}"
                                                         VerticalContentAlignment="Center"
                                                         Tag="Search Workspace"
                                                         TextSearch.Text="search"
                                                         Height="35"
                                                         VerticalAlignment="Top"
                                                         FontFamily="/Luna X;component/Graphics/Fonts/#Poppins Medium"
                                                         Foreground="White"
                                                         Background="Transparent"
                                                         TextChanged="WorkspaceSearch_TextChanged"
                                                         BorderThickness="0"
                                                         BorderBrush="Transparent"/>

                                                <TreeView x:Name="Workspace"
                                                          Grid.Row="1"
                                                          Margin="0 37 0 0"
                                                          VirtualizingPanel.IsVirtualizing="True"
                                                          VirtualizingPanel.VirtualizationMode="Recycling"
                                                          ItemsSource="{Binding RootItems}"
                                                          Background="Transparent"
                                                          BorderBrush="Transparent"
                                                          Foreground="White"
                                                          BorderThickness="0"
                                                          SelectedItemChanged="Workspace_SelectedItemChanged">

                                                    <!-- File Templates -->
                                                    <TreeView.Resources>
                                                        <HierarchicalDataTemplate DataType="{x:Type local:FolderItem}" ItemsSource="{Binding Children}">
                                                            <StackPanel Orientation="Horizontal">
                                                                <TextBlock Text="folder" FontSize="16" Margin="0,0,5,0"
                                                                           FontFamily="/Luna X;component/Graphics/Fonts/Icon/#Material Icons Round" Foreground="LightGray"/>
                                                                <TextBlock Text="{Binding Header}" VerticalAlignment="Center" Foreground="White" FontFamily="/Luna X;component/Graphics/Fonts/#Poppins Medium" />
                                                            </StackPanel>
                                                        </HierarchicalDataTemplate>
                                                        <!-- Lua File Template -->
                                                        <DataTemplate DataType="{x:Type local:LuaFileItem}">
                                                            <StackPanel Orientation="Horizontal">
                                                                <Image Source="/Luna X;component/Graphics/Images/Lua.png"
                                                                       Width="16"
                                                                       Height="16" 
                                                                       Margin="0,0,5,0"
                                                                       RenderOptions.BitmapScalingMode="HighQuality"/>
                                                                <TextBlock Text="{Binding Header}" VerticalAlignment="Center" Foreground="White" FontFamily="/Luna X;component/Graphics/Fonts/#Poppins Medium"/>
                                                            </StackPanel>
                                                        </DataTemplate>

                                                        <!-- Text File Template -->
                                                        <DataTemplate DataType="{x:Type local:TextFileItem}">
                                                            <StackPanel Orientation="Horizontal">
                                                                <TextBlock Text="&#xe7c3;"
                                                                           FontFamily="/Luna X;component/Graphics/Fonts/Icon/#Segoe Fluent Icons" 
                                                                           FontSize="16"
                                                                           Margin="0,0,5,0"
                                                                           Foreground="LightGray"/>
                                                                <TextBlock Text="{Binding Header}" VerticalAlignment="Center" Foreground="White" FontFamily="/Luna X;component/Graphics/Fonts/#Poppins Medium"/>
                                                            </StackPanel>
                                                        </DataTemplate>


                                                        <!-- Other File Template -->
                                                        <DataTemplate DataType="{x:Type local:OtherFileItem}">
                                                            <StackPanel Orientation="Horizontal">
                                                                <TextBlock Text="code" 
                                                                           FontSize="16" 
                                                                           Margin="0,0,5,0" 
                                                                           FontFamily="/Luna X;component/Graphics/Fonts/Icon/#Material Icons Round" Foreground="LightGray"/>

                                                                <TextBlock Text="{Binding Header}" VerticalAlignment="Center" Foreground="White" FontFamily="/Luna X;component/Graphics/Fonts/#Poppins Medium"/>
                                                            </StackPanel>
                                                        </DataTemplate>
                                                    </TreeView.Resources>
                                                </TreeView>
                                            </Grid>

                                        </Border>


                                    <Border x:Name="ExecButtons"
                                            VerticalAlignment="Bottom"
                                            HorizontalAlignment="Right"
                                                Height="35"
                                            BorderThickness="1"
                                            Margin="5 5"
                                            BorderBrush="#5F41434A"
                                            Background="#16383838"
                                            CornerRadius="6">

                                        <Border.Resources>
                                            <Style TargetType="Label">
                                                <Setter Property="IsHitTestVisible" Value="False"/>
                                            </Style>
                                        </Border.Resources>

                                        <StackPanel Margin="5,0"
                                                    FlowDirection="LeftToRight"
                                                    Height="35"
                                                    Orientation="Horizontal">

                                            <Button x:Name="Execute"
                                                    Background="Transparent"
                                                    BorderBrush="Transparent"
                                                    BorderThickness="0"
                                                    VerticalAlignment="Center"
                                                    Height="35"
                                                    Width="35"
                                                    Padding="2 0 0 1"
                                                    Click="Execute_Click"
                                                    IsHitTestVisible="True"
                                                    materialdesign:ButtonAssist.CornerRadius="0"
                                                    ToolTip="Execute Editor Code"
                                                    ToolTipService.HasDropShadow="True"
                                                    ToolTipService.InitialShowDelay="500">
                                                <Label Content="&#xf04b;"
                                                       FontFamily="/Luna X;component/Graphics/Fonts/Icon/#Font Awesome 6 Free Solid"
                                                       Foreground="{DynamicResource Accent}"
                                                       Width="30"
                                                       Height="30"
                                                       HorizontalAlignment="Left"
                                                       VerticalAlignment="Center"
                                                       VerticalContentAlignment="Center"
                                                       TextOptions.TextFormattingMode="Ideal"
                                                       HorizontalContentAlignment="Center"
                                                       FontSize="18">
                                                     <Label.Effect>
                                                        <DropShadowEffect Color="{DynamicResource AccentSingular}" 
                                                                          BlurRadius="6"
                                                                          ShadowDepth="0" 
                                                                          Opacity="0.5"/>
                                                    </Label.Effect>

                                                </Label>

                                            </Button>

                                            <Button x:Name="Clients"
                                                    Background="Transparent"
                                                    BorderBrush="Transparent"
                                                    BorderThickness="0"
                                                    VerticalAlignment="Center"
                                                    IsHitTestVisible="True"
                                                    Height="35"
                                                    Width="35"
                                                    Click="Clients_Click"
                                                    Padding="2 0 0 1"
                                                    materialdesign:ButtonAssist.CornerRadius="0"
                                                    ToolTip="Open Client Instance Manager"
                                                    ToolTipService.HasDropShadow="True"
                                                    ToolTipService.InitialShowDelay="500">
                                                <Label Content="&#xf5fd;"
                                                           FontFamily="/Luna X;component/Graphics/Fonts/Icon/#Font Awesome 6 Free Solid"
                                                           Foreground="{DynamicResource AccentTransparent}"
                                                           Width="30"
                                                           Height="30"
                                                           HorizontalAlignment="Left"
                                                           VerticalAlignment="Center"
                                                           VerticalContentAlignment="Center"
                                                           HorizontalContentAlignment="Center"
                                                           FontSize="18">
                                                     <Label.Effect>
                                                        <DropShadowEffect Color="{DynamicResource AccentSingular}" 
                                                                          BlurRadius="6"
                                                                          ShadowDepth="0" 
                                                                          Opacity="0.5"/>
                                                    </Label.Effect>

                                                </Label>


                                            </Button>

                                            <Button x:Name="Open"
                                                    Background="Transparent"
                                                    BorderBrush="Transparent"
                                                    BorderThickness="0"
                                                    VerticalAlignment="Center"
                                                    IsHitTestVisible="True"
                                                    Height="35"
                                                    Width="35"
                                                    Padding="2 0 0 1"
                                                    Click="Open_Click"
                                                    ToolTip="Open/Import File"
                                                    ToolTipService.InitialShowDelay="500"
                                                    ToolTipService.HasDropShadow="True"
                                                    materialdesign:ButtonAssist.CornerRadius="0">
                                                <Label Content="&#xf56f;"
                                                           FontFamily="/Luna X;component/Graphics/Fonts/Icon/#Font Awesome 6 Free Solid"
                                                           Foreground="#CfFFFFFF"
                                                           Width="30"
                                                           Height="30"
                                                           HorizontalAlignment="Left"
                                                           VerticalAlignment="Center"
                                                           VerticalContentAlignment="Center"
                                                           HorizontalContentAlignment="Center"
                                                           FontSize="18"/>
                                                

                                            </Button>

                                            <Button x:Name="Clear"
                                                    Background="Transparent"
                                                    BorderBrush="Transparent"
                                                    BorderThickness="0"
                                                    VerticalAlignment="Center"
                                                    Height="35"
                                                    IsHitTestVisible="True"
                                                    Padding="2 0 0 1"
                                                    Width="35"
                                                    ToolTip="Clear Text Editor"
                                                    Click="Clear_Click"
                                                    ToolTipService.InitialShowDelay="500"
                                                    ToolTipService.HasDropShadow="True"
                                                    materialdesign:ButtonAssist.CornerRadius="0">
                                                <Label Content="trash"
                                                           FontFamily="/Luna X;component/Graphics/Fonts/Icon/#Font Awesome 6 Free Solid"
                                                           Foreground="#CfFFFFFF"
                                                           Width="30"
                                                           Height="30"
                                                           HorizontalAlignment="Left"
                                                           VerticalAlignment="Center"
                                                           VerticalContentAlignment="Center"
                                                           HorizontalContentAlignment="Center"
                                                           FontSize="18"/>

                                            </Button>

                                            <Button x:Name="Export"
                                                    Background="Transparent"
                                                    BorderBrush="Transparent"
                                                    BorderThickness="0"
                                                    IsHitTestVisible="True"
                                                    VerticalAlignment="Center"
                                                    Padding="2 0 0 1"
                                                    Height="35"
                                                    Width="35"
                                                    Click="Export_Click"
                                                    materialdesign:ButtonAssist.CornerRadius="0"
                                                    ToolTip="Save/Export File"
                                                    ToolTipService.HasDropShadow="True"
                                                    ToolTipService.InitialShowDelay="500">
                                                <Label Content="&#xf56e;"
                                                           FontFamily="/Luna X;component/Graphics/Fonts/Icon/#Font Awesome 6 Free Solid"
                                                           Foreground="#CfFFFFFF"
                                                           Width="30"
                                                           Height="30"
                                                           HorizontalAlignment="Left"
                                                           VerticalAlignment="Center"
                                                           VerticalContentAlignment="Center"
                                                           HorizontalContentAlignment="Center"
                                                           FontSize="18"/>
                                            </Button>

                                            <Button x:Name="Extra"
                                                    Background="Transparent"
                                                    BorderBrush="Transparent"
                                                    BorderThickness="0"
                                                    Click="Extra_Click"
                                                    VerticalAlignment="Center"
                                                    Height="35"
                                                    Padding="0 0 0 0"
                                                    Width="30"
                                                    materialdesign:ButtonAssist.CornerRadius="0"
                                                    ToolTip="View More"
                                                    ToolTipService.HasDropShadow="True"
                                                    ToolTipService.InitialShowDelay="500">

                                                <Grid>
                                                    <!--f053-->
                                                    <Label x:Name="ViewMoreLabel"
                                                           Content="&#xf054;"
                                                           FontFamily="/Luna X;component/Graphics/Fonts/Icon/#Font Awesome 6 Free Solid"
                                                           Foreground="#CfFFFFFF"
                                                           Width="30"
                                                           Height="30"
                                                           Padding="0 0 0 1"
                                                           HorizontalAlignment="Left"
                                                           VerticalAlignment="Center"
                                                           VerticalContentAlignment="Center"
                                                           HorizontalContentAlignment="Center"
                                                           FontSize="16"/>

                                                    <StackPanel Margin="30,0,0,0"
                                                                FlowDirection="LeftToRight"
                                                                Orientation="Horizontal">

                                                        <Button x:Name="ExecFile"
                                                                Background="Transparent"
                                                                BorderBrush="Transparent"
                                                                BorderThickness="0"
                                                                VerticalAlignment="Center"
                                                                Height="35"
                                                                Padding="2 0 0 1"
                                                                Click="ExecFile_Click"
                                                                Width="35"
                                                                materialdesign:ButtonAssist.CornerRadius="0"
                                                                ToolTip="Execute File Content"
                                                                ToolTipService.HasDropShadow="True"
                                                                ToolTipService.InitialShowDelay="500">

                                                            <Grid>
                                                                <Label Content="file"
                                                                       FontFamily="/Luna X;component/Graphics/Fonts/Icon/#Font Awesome 6 Free Solid"
                                                                       Foreground="#CFFCFCFC"
                                                                       Width="30"
                                                                       Height="30"
                                                                       Padding="0 1 3 0"
                                                                       HorizontalAlignment="Left"
                                                                       VerticalAlignment="Center"
                                                                       VerticalContentAlignment="Center"
                                                                       HorizontalContentAlignment="Center"
                                                                       FontSize="18"/>

                                                                <Label Content="play"
                                                                       FontFamily="/Luna X;component/Graphics/Fonts/Icon/#Font Awesome 6 Free Solid"
                                                                       Foreground="#DfFFFFFF"
                                                                       Width="30"
                                                                       Height="30"
                                                                       Padding="8 10 0 0"
                                                                       HorizontalAlignment="Left"
                                                                       VerticalAlignment="Center"
                                                                       VerticalContentAlignment="Center"
                                                                       HorizontalContentAlignment="Center"
                                                                       FontSize="13"/>
                                                            </Grid>

                                                        </Button>


                                                        <Button x:Name="Copy"
                                                                Background="Transparent"
                                                                BorderBrush="Transparent"
                                                                BorderThickness="0"
                                                                VerticalAlignment="Center"
                                                                Height="35"
                                                                Width="35"
                                                                Padding="2 0 0 1"
                                                                Click="Copy_Click"
                                                                materialdesign:ButtonAssist.CornerRadius="0"
                                                                ToolTip="Copy Editor Content"
                                                                ToolTipService.HasDropShadow="True"
                                                                ToolTipService.InitialShowDelay="500">
                                                            <Label Content="&#xe14d;"
                                                                       FontFamily="/Luna X;component/Graphics/Fonts/Icon/#Material Icons Round"
                                                                       Foreground="#CfFFFFFF"
                                                                       Width="30"
                                                                       Height="30"
                                                                       HorizontalAlignment="Left"
                                                                       VerticalAlignment="Center"
                                                                       VerticalContentAlignment="Center"
                                                                       HorizontalContentAlignment="Center"
                                                                       FontSize="18"/>

                                                        </Button>

                                                        <Button x:Name="Paste"
                                                                Background="Transparent"
                                                                BorderBrush="Transparent"
                                                                BorderThickness="0"
                                                                VerticalAlignment="Center"
                                                                Height="35"
                                                                Width="35"
                                                                Padding="2 0 0 1"
                                                                Click="Paste_Click"
                                                                materialdesign:ButtonAssist.CornerRadius="0"
                                                                ToolTip="Paste Into Editor"
                                                                ToolTipService.HasDropShadow="True"
                                                                ToolTipService.InitialShowDelay="500">

                                                            <Label Content="clipboard"
                                                                       FontFamily="/Luna X;component/Graphics/Fonts/Icon/#Font Awesome 6 Free Solid"
                                                                       Foreground="#CfFFFFFF"
                                                                       Width="30"
                                                                       Height="30"
                                                                       HorizontalAlignment="Left"
                                                                       VerticalAlignment="Center"
                                                                       VerticalContentAlignment="Center"
                                                                       HorizontalContentAlignment="Center"
                                                                       FontSize="18"/>
                                                        </Button>

                                                        <Button x:Name="Undo"
                                                                Background="Transparent"
                                                                BorderBrush="Transparent"
                                                                BorderThickness="0"
                                                                VerticalAlignment="Center"
                                                                Height="35"
                                                                Width="35"
                                                                Padding="2 0 0 1"
                                                                Click="Undo_Click"
                                                                materialdesign:ButtonAssist.CornerRadius="0"
                                                                ToolTip="Undo Action"
                                                                ToolTipService.HasDropShadow="True"
                                                                ToolTipService.InitialShowDelay="500">

                                                            <Label Content="&#xf0e2;"
                                                                       FontFamily="/Luna X;component/Graphics/Fonts/Icon/#Font Awesome 6 Free Solid"
                                                                       Foreground="#CfFFFFFF"
                                                                       Width="30"
                                                                       Height="30"
                                                                       HorizontalAlignment="Left"
                                                                       VerticalAlignment="Center"
                                                                       VerticalContentAlignment="Center"
                                                                       HorizontalContentAlignment="Center"
                                                                       FontSize="18"/>

                                                        </Button>

                                                        <Button x:Name="Redo"
                                                                Background="Transparent"
                                                                BorderBrush="Transparent"
                                                                BorderThickness="0"
                                                                VerticalAlignment="Center"
                                                                Height="35"
                                                                Width="35"
                                                                Padding="2 0 0 1"
                                                                Click="Redo_Click"
                                                                materialdesign:ButtonAssist.CornerRadius="0"
                                                                ToolTip="Redo Action"
                                                                ToolTipService.HasDropShadow="True"
                                                                ToolTipService.InitialShowDelay="500">

                                                                <Label Content="&#xf01e;"
                                                                       FontFamily="/Luna X;component/Graphics/Fonts/Icon/#Font Awesome 6 Free Solid"
